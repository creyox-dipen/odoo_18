<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="find_duplicate_data_server_action_id" model="ir.actions.server">
        <field name="name">Find Duplicate Data</field>
        <field name="model_id" ref="cr_merge_duplicate_data_extended.model_res_partner"/>
        <field name="binding_model_id" ref="base.model_res_partner"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
            action = records.find_duplicate_data()
        </field>
    </record>

    <record id="merge_duplicate_data_server_action_id" model="ir.actions.server">
        <field name="name">Merge Duplicate Data</field>
        <field name="model_id" ref="cr_merge_duplicate_data_extended.model_res_partner"/>
        <field name="binding_model_id" ref="base.model_res_partner"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
            action = records.merge_duplicate_data()
        </field>
        <field name="groups_id" eval="[(4, ref('cr_merge_duplicate_data_extended.merge_data_group_id'))]"/>
    </record>
</odoo>